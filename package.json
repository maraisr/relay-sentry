{
	"name": "relay-sentry",
	"version": "0.0.5",
	"description": "Relay log function that enriches Sentry with Relay life cycles and GraphQL data",
	"keywords": [
		"relay",
		"graphql",
		"sentry",
		"error"
	],
	"repository": "maraisr/relay-sentry",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"exports": {
		".": [
			{
				"import": "./dist/index.esm.js",
				"require": "./dist/index.js"
			},
			"./dist/index.js"
		]
	},
	"main": "dist/index.js",
	"unpkg": "dist/index.min.js",
	"module": "dist/index.esm.js",
	"types": "types/index.d.ts",
	"files": [
		"dist",
		"types"
	],
	"scripts": {
		"bench": "node -r ts-node/register bench/index.ts",
		"build": "rollup -c",
		"format": "prettier \"{*,tests/*,src/*,.github/**/*}.+(ts|js|yml|md)\" --write",
		"prepublishOnly": "yarn build",
		"test": "uvu -r ts-node/register tests"
	},
	"prettier": {
		"bracketSpacing": true,
		"printWidth": 80,
		"proseWrap": "always",
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"@sentry/minimal": "^5.27.2",
		"@sentry/types": "^5.27.2"
	},
	"devDependencies": {
		"@marais/tsconfig": "^0.0.1",
		"@rollup/plugin-node-resolve": "10.0.0",
		"@sentry/node": "^5.27.2",
		"@types/relay-runtime": "^10",
		"@types/relay-test-utils": "^6.0.3",
		"graphql": "^15.4",
		"prettier": "^2.0.5",
		"relay-runtime": "^10.0.1",
		"relay-test-utils": "^10.0.1",
		"relay-test-utils-internal": "^10.0.1",
		"rollup": "2.33.1",
		"rollup-plugin-terser": "7.0.2",
		"rollup-plugin-typescript2": "0.29.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.1.0-dev.20201102",
		"uvu": "^0.3.0"
	},
	"peerDependencies": {
		"@types/relay-runtime": "^10.0.10",
		"graphql": "^15.4"
	},
	"peerDependenciesMeta": {
		"@types/relay-runtime": {
			"optional": true
		},
		"graphql": {
			"optional": true
		}
	},
	"engines": {
		"node": ">=12"
	},
	"volta": {
		"node": "15.0.1",
		"yarn": "1.22.5"
	}
}
